{
    "name": "dive-v3-backend",
    "version": "1.0.0",
    "description": "DIVE V3 Backend API with PEP for ABAC authorization",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/https-server.ts",
        "dev:http": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/https-server.js",
        "test": "NODE_ENV=test jest --testTimeout=15000 --runInBand",
        "test:unit": "NODE_ENV=test jest --testPathIgnorePatterns=integration --testTimeout=15000 --maxWorkers=1 --config=jest.config.js --testPathPattern=\"src/__tests__/mongodb-config.test.ts|src/__tests__/circuit-breaker.test.ts|src/__tests__/rate-limit.middleware.test.ts\"",
        "test:integration": "NODE_ENV=test jest --testPathPattern=integration --testTimeout=15000 --runInBand",
        "test:audit-logs": "NODE_ENV=test jest --testPathPattern=audit-log --testTimeout=15000 --runInBand",
        "test:watch": "NODE_ENV=test jest --watch",
        "test:coverage": "NODE_ENV=test jest --coverage --testTimeout=15000 --maxWorkers=50%",
        "test:ci": "jest --config jest.config.ci.js --coverage --ci",
        "test:performance": "jest --testTimeout=45000 --maxWorkers=1 --verbose --runInBand",
        "test:federation": "NODE_ENV=test jest --testPathPattern=federated-search --testTimeout=15000",
        "test:federation:e2e": "RUN_E2E_TESTS=true NODE_ENV=test jest --testPathPattern=federated-search.e2e --runInBand --testTimeout=30000",
        "test:federation:perf": "RUN_E2E_TESTS=true NODE_ENV=test jest --testPathPattern=federated-search --testNamePattern=Performance --runInBand --testTimeout=60000",
        "lint": "eslint src/**/*.ts",
        "lint:coi": "tsx src/scripts/coi-logic-lint.ts",
        "purge:invalid": "tsx src/scripts/purge-invalid-coi.ts",
        "typecheck": "tsc --noEmit",
        "seed-database": "tsx src/scripts/seed-7000-ztdf-documents.ts",
        "seed-ztdf": "tsx src/scripts/seed-ztdf-resources.ts",
        "seed:fixed": "tsx src/scripts/seed-1000-ztdf-documents-fixed.ts",
        "seed:instance": "tsx src/scripts/seed-instance-resources.ts",
        "seed:all": "tsx src/scripts/seed-instance-resources.ts -- --instance=ALL",
        "seed:usa": "tsx src/scripts/seed-instance-resources.ts -- --instance=USA --count=7000",
        "seed:fra": "tsx src/scripts/seed-instance-resources.ts -- --instance=FRA --count=7000",
        "seed:gbr": "tsx src/scripts/seed-instance-resources.ts -- --instance=GBR --count=7000",
        "seed:deu": "tsx src/scripts/seed-instance-resources.ts -- --instance=DEU --count=7000",
        "migrate-logs": "tsx src/scripts/migrate-logs-to-mongodb.ts",
        "migrate:classification-equivalency": "tsx src/scripts/migrate-classification-equivalency.ts",
        "migrate:classification-equivalency:execute": "tsx src/scripts/migrate-classification-equivalency.ts -- --execute",
        "migrate:classification-equivalency:rollback": "tsx src/scripts/migrate-classification-equivalency.ts --",
        "migrate:coi-capitalization": "tsx src/scripts/migrate-coi-capitalization.ts",
        "migrate:coi-capitalization:dry-run": "tsx src/scripts/migrate-coi-capitalization.ts --dry-run",
        "optimize-database": "tsx src/scripts/optimize-database.ts",
        "create-text-indexes": "tsx scripts/create-text-indexes.ts",
        "create-text-indexes:usa": "tsx scripts/create-text-indexes.ts USA",
        "create-text-indexes:all": "tsx scripts/create-text-indexes.ts USA FRA GBR DEU",
        "generate-certs": "tsx src/scripts/generate-certificates.ts",
        "generate-ca": "tsx src/scripts/generate-three-tier-ca.ts",
        "generate-real-x509-ca": "tsx src/scripts/generate-real-x509-ca.ts",
        "generate-test-keys": "chmod +x scripts/generate-test-rsa-keys.sh && ./scripts/generate-test-rsa-keys.sh",
        "migrate-uuids": "tsx src/scripts/migrate-uniqueids-to-uuid.ts",
        "fix-webauthn-rpid": "tsx src/scripts/fix-webauthn-rpid.ts",
        "fix-webauthn-warnings": "tsx src/scripts/fix-webauthn-warnings.ts",
        "quick-fix-acr": "tsx src/scripts/quick-fix-acr-aal3.ts",
        "emergency-set-acr": "tsx src/scripts/emergency-set-acr-aal3.ts",
        "debug-webauthn": "tsx src/scripts/debug-webauthn-passkey.ts",
        "delete-webauthn": "tsx src/scripts/delete-and-fix-webauthn.ts",
        "fix-webauthn-lookup": "tsx src/scripts/fix-webauthn-server-side-lookup.ts",
        "check-webauthn": "tsx src/scripts/check-and-clean-webauthn-credentials.ts",
        "verify-webauthn": "tsx src/scripts/verify-webauthn-config.ts",
        "verify-both-policies": "tsx src/scripts/verify-both-webauthn-policies.ts",
        "check-webauthn-comprehensive": "tsx src/scripts/comprehensive-webauthn-check.ts",
        "clear-sessions": "tsx src/scripts/clear-keycloak-sessions.ts",
        "check-webauthn-credentials": "tsx src/scripts/check-and-clean-webauthn-credentials.ts",
        "setup-demo-users": "tsx src/scripts/setup-demo-users.ts",
        "test-demo-users": "tsx src/scripts/test-demo-users.ts",
        "get-demo-otp": "tsx src/scripts/get-demo-otp-code.ts",
        "opal-publisher": "tsx src/scripts/opal-publisher.ts",
        "opal:sync": "tsx src/scripts/opal-publisher.ts -- --sync",
        "opal:watch": "tsx src/scripts/opal-publisher.ts -- --watch",
        "opal:health": "tsx src/scripts/opal-publisher.ts -- --health"
    },
    "keywords": [
        "dive",
        "icam",
        "abac",
        "opa",
        "keycloak",
        "nato"
    ],
    "author": "DIVE V3 Team",
    "license": "MIT",
    "dependencies": {
        "@keycloak/keycloak-admin-client": "^26.4.0",
        "@types/jszip": "^3.4.0",
        "@types/speakeasy": "^2.0.10",
        "@types/uuid": "^10.0.0",
        "axios": "^1.6.2",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "rate-limit-redis": "^4.2.0",
        "express-validator": "^7.2.1",
        "google-auth-library": "^10.5.0",
        "helmet": "^7.1.0",
        "ioredis": "^5.3.2",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.2",
        "jszip": "^3.10.1",
        "jwk-to-pem": "^2.0.7",
        "jwks-rsa": "^3.1.0",
        "mongodb": "^6.3.0",
        "node-cache": "^5.1.2",
        "node-forge": "^1.3.1",
        "prom-client": "^15.1.3",
        "qrcode": "^1.5.3",
        "speakeasy": "^2.0.0",
        "uuid": "^11.1.0",
        "winston": "^3.11.0",
        "xml2js": "^0.6.2",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/jwk-to-pem": "^2.0.3",
        "@types/multer": "^2.0.0",
        "@types/node": "^20.10.6",
        "@types/node-forge": "^1.3.14",
        "@types/qrcode": "^1.5.6",
        "@types/supertest": "^6.0.3",
        "@types/xml2js": "^0.4.14",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "ioredis-mock": "^8.9.0",
        "jest": "^29.7.0",
        "mongodb-memory-server": "^9.5.0",
        "multer": "^1.4.5-lts.1",
        "nock": "^13.5.0",
        "prettier": "^3.1.1",
        "supertest": "^7.1.4",
        "ts-jest": "^29.1.1",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    }
}
