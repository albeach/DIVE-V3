{
    "name": "dive-v3-frontend",
    "version": "1.0.0",
    "description": "DIVE V3 Frontend - Coalition ICAM Pilot",
    "private": true,
    "overrides": {
        "swagger-ui-react": {
            "react": "$react",
            "react-dom": "$react-dom"
        }
    },
    "scripts": {
        "dev": "node server.js",
        "dev:http": "next dev",
        "build": "SKIP_ENV_VALIDATION=1 next build",
        "start": "next start",
        "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx",
        "typecheck": "tsc --noEmit",
        "db:generate": "drizzle-kit generate",
        "db:push": "drizzle-kit push",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:e2e": "playwright test",
        "test:e2e:fast": "TEST_TAG=@fast playwright test",
        "test:e2e:smoke": "TEST_TAG=@smoke playwright test",
        "test:e2e:critical": "TEST_TAG=@critical playwright test",
        "test:e2e:flaky": "TEST_TAG=@flaky playwright test --retries=3",
        "test:e2e:animations": "playwright test src/__tests__/e2e/animations --reporter=html --reporter=list",
        "test:e2e:animations:ui": "playwright test src/__tests__/e2e/animations --ui",
        "test:e2e:animations:debug": "playwright test src/__tests__/e2e/animations --debug",
        "test:e2e:localhost": "BASE_URL=http://localhost:3000 playwright test",
        "test:e2e:hub": "playwright test --project=hub-chromium",
        "test:e2e:spoke": "playwright test --project=spoke-fra-chromium --project=spoke-gbr-chromium",
        "test:e2e:federation": "playwright test --project=federation-chromium",
        "test:e2e:all-instances": "npm run test:e2e:hub && npm run test:e2e:spoke",
        "test:e2e:dynamic": "../scripts/dynamic-test-runner.sh",
        "test:e2e:dynamic:parallel": "../scripts/dynamic-test-runner.sh --parallel",
        "test:e2e:dynamic:federation": "../scripts/dynamic-test-runner.sh --federation",
        "test:e2e:dynamic:dry-run": "../scripts/dynamic-test-runner.sh --dry-run --verbose",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:report": "playwright show-report",
        "test:e2e:debug": "playwright test --debug",
        "test:performance": "playwright test --grep performance",
        "test:performance:auth": "playwright test tests/e2e/auth-flows.test.ts --grep 'performance'",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@auth/drizzle-adapter": "^1.11.1",
        "@headlessui/react": "^2.2.9",
        "@heroicons/react": "^2.2.0",
        "@hookform/resolvers": "^5.2.2",
        "@radix-ui/react-accordion": "^1.2.12",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-visually-hidden": "^1.2.4",
        "@tanstack/react-query": "^5.90.5",
        "@tanstack/react-virtual": "^3.13.18",
        "@types/qrcode": "^1.5.6",
        "@types/speakeasy": "^2.0.10",
        "@types/swagger-ui-react": "^5.18.0",
        "axios": "^1.7.0",
        "clsx": "^2.1.1",
        "cmdk": "^1.1.1",
        "date-fns": "^3.6.0",
        "drizzle-orm": "^0.45.1",
        "framer-motion": "^11.18.2",
        "fuse.js": "^7.1.0",
        "jszip": "^3.10.1",
        "jwt-decode": "^4.0.0",
        "lottie-react": "^2.4.1",
        "lucide-react": "^0.546.0",
        "next": "^16.1.6",
        "next-auth": "^5.0.0-beta.25",
        "next-themes": "^0.4.6",
        "postgres": "^3.4.4",
        "pptxgenjs": "^4.0.1",
        "prism-react-renderer": "^2.4.1",
        "qrcode": "^1.5.4",
        "qrcode.react": "^4.2.0",
        "react": "^19.2.3",
        "react-confetti": "^6.4.0",
        "react-dom": "^19.2.3",
        "react-dropzone": "^14.3.8",
        "react-hook-form": "^7.66.0",
        "react-hot-toast": "^2.6.0",
        "react-is": "^19.1.1",
        "react-player": "^3.4.0",
        "reactflow": "^11.11.4",
        "recharts": "^3.2.1",
        "sonner": "^2.0.7",
        "speakeasy": "^2.0.0",
        "swagger-ui-react": "^5.31.0",
        "tailwind-merge": "^3.4.0",
        "wavesurfer.js": "^7.12.1",
        "xlsx": "^0.18.5",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@axe-core/playwright": "^4.11.1",
        "@playwright/test": "^1.57.0",
        "@storybook/nextjs-vite": "^10.2.7",
        "@swc/jest": "^0.2.39",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^24.10.4",
        "@types/react": "^18.3.0",
        "@types/react-dom": "^18.3.0",
        "autoprefixer": "^10.4.16",
        "ci-info": "^4.3.1",
        "dotenv": "^16.6.1",
        "drizzle-kit": "^0.31.5",
        "eslint": "^9.0.0",
        "eslint-config-next": "15.0.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "postcss": "^8.4.32",
        "storybook": "^10.2.7",
        "tailwindcss": "^3.4.0",
        "typescript": "5.9.3"
    }
}
