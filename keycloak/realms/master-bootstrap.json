{
  "id": "master",
  "realm": "master",
  "displayName": "Keycloak",
  "enabled": true,
  "sslRequired": "external",
  "clients": [
    {
      "clientId": "admin-cli",
      "enabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "implicitFlowEnabled": false,
      "standardFlowEnabled": false,
      "publicClient": true
    }
  ],
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Admin",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "${KEYCLOAK_ADMIN_PASSWORD}",
          "temporary": false
        }
      ],
      "realmRoles": ["admin"],
      "clientRoles": {}
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "admin",
        "composite": false,
        "clientRole": false,
        "containerId": "master"
      }
    ]
  }
}